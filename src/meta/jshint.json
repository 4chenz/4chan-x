  "sub":     true,
  "scripturl": true,
  "browser": true,
  "devel":   true,
  "nonstandard": true,
  "-W018": true,
  "-W084": true,
  "-W083": true,
  "-W093": true,
  "globals": {
    "MediaError":   true,
